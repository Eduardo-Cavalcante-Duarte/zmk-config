#include "../la_usurpadora/la_usurpadora.dtsi"

/ {
    /delete-node/ kscan;
};

/ {
    chosen {
        zmk,kscan = &mock_kscan;
        zmk,physical-layout = &default_layout;
    };

    mock_kscan: mock_kscan {
        compatible = "zmk,kscan-mock";
        columns = <14>; 
        rows = <6>;
        events = <0>;
    };
};

&default_layout {
    kscan = <&mock_kscan>;
};
